#printing from n to 1:
n = int(input("n:"))
i=n
while(i>0):
    print(i)
    i=i-1
    
    
    
#printing sum of first n natural numbers:
n = int(input("n:"))
sum=0
i=0
while(i<=n):
    sum=sum+i
    i=i+1
print(sum)




#printing sum of square of first n natural numbers
n = int(input("n:"))
sum=0
i=0
while(i<n):
    i=i+1
    j=i*i
    sum=sum+j
print(sum)



#printing sum of cube of first n natural numbers:
n= int(input("n:"))
sum=0
i=0
while(i<n):
    i=i+1
    j=i*i*i
    sum=sum+j
print(sum)



#printing sum of even numbers from 1 to n
n = int(input("n:"))
sum=0
i=0
while(i<n):
    i=i+2
    sum=sum+i
print(sum)


#printing sum of digit of a given numbers
i = int(input("n:"))
sum=0
while(i>0):
    j=i%10
    sum=sum+j
    i=i//10
print(sum)


#printing sum of square of digits in a given number
i = int(input("n:"))
sum=0
while(i>0):
    j=i%10
    k=j*j
    sum=sum+k
    i=i//10
print(sum)

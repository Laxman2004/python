#printing from 1 to 10
i = 0
while(i<10):
    i=i+1
    print(i)



#printing even numbers from 1 to 10
i = 2
while(i<10):
    i=i+2
    print(i)




#printing from 10 to 1
i = 11
while(i>1):
    i=i-1
    print(i)




#printing odd numbers from 1 to 100
i = 0
while(i<100):
    i=i+1
    if i%2!=0:
        print(i)"""




